# This file is for unifying the coding style for different editors and IDEs

root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = tab
indent_size = 4
quote_type = single
max_line_length = 120

[{*.html}]
indent_style = space
indent_size = 4

[{*.txt}]
end_of_line = crlf
